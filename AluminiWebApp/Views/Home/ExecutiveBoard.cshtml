@model  AluminiWebApp.Areas.Admin.Models.ExecutiveBoard
@{
    Layout = "~/Views/Shared/_SRKRDesignLayout.cshtml";
}

<section class="nicdark_section nicdark_imgparallax nicdark_parallaxx_img6" id="nicdark_parallax_title" style="background-position: 50% 0px;">
    <div class="nicdark_filter greydark">
        <!--start nicdark_container-->
        <div class="nicdark_container nicdark_clearfix">
            <div class="grid grid_12">
                <div class="nicdark_space100"></div>
                <div class="nicdark_space100"></div>
                <h1 class="white subtitle">Coordination Committee Members</h1>
                <div class="nicdark_space10"></div>
                <h3 class="subtitle white"></h3>
                <div class="nicdark_space20"></div>
                <div class="nicdark_divider left big"><span class="nicdark_bg_white nicdark_radius"></span></div>
                <div class="nicdark_space40"></div>
                <div class="nicdark_space50"></div>
            </div>
        </div>
        <!--end nicdark_container-->
    </div>

</section>

<section class="nicdark_section executive-board">
    <!--start nicdark_container-->
    <div class="nicdark_container nicdark_clearfix">

        <div class="nicdark_space50"></div>

        <div class="grid grid_12">
            <h1 class="subtitle greydark">Coordination Committee Members</h1>
            <div class="nicdark_space20"></div>
            <h3 class="subtitle grey"></h3>
            <div class="nicdark_space20"></div>
            <div class="nicdark_divider left big"><span class="nicdark_bg_blue nicdark_radius"></span></div>
            <div class="nicdark_space10"></div>
        </div>
        @foreach (var Data in @Model.ExecutiveBoardDetail)
        {
            <div class="grid grid_3">

                <div class="nicdark_archive1 nicdark_bg_grey nicdark_radius nicdark_shadow center">
                    <div class="nicdark_textevidence nicdark_bg_greydark nicdark_radius_top">
                        <h4 class="white nicdark_margin20">@Data.Name</h4>
                    </div>
                    <img src="@Data.Image" alt="" class="nicdark_opacity">
                    <div class="nicdark_textevidence nicdark_bg_blue">
                        <h5 class="white nicdark_margin20">@Data.ROle</h5>
                        <i class="icon-brush nicdark_iconbg right medium blue"></i>
                    </div>

                    <div class="nicdark_textevidence">
                        <div class="nicdark_margin10">
                            <p>@Data.Email</p>
                            <p>@Data.Mobile</p>
                            <div class="nicdark_space20 nicdark_displaynone_ipadpotr"></div>

                            @*<a class="nicdark_displaynone_ipadpotr nicdark_btn_icon nicdark_tooltip nicdark_bg_blue medium nicdark_shadow nicdark_radius_circle white nicdark_margin010" href="#" title="CURRICULUM"><i class="icon-download-outline nicdark_rotate"></i></a>
                                <a class="nicdark_displaynone_ipadpotr nicdark_btn_icon nicdark_tooltip nicdark_bg_blue medium nicdark_shadow nicdark_radius_circle white nicdark_margin010" href="#" title="DOCUMENTS"><i class="icon-attach-outline nicdark_rotate"></i></a>
                                <a class="nicdark_displaynone_ipadpotr nicdark_btn_icon nicdark_tooltip nicdark_bg_blue medium nicdark_shadow nicdark_radius_circle white nicdark_margin010" href="#" title="COURSES"><i class="icon-mic-outline nicdark_rotate"></i></a>*@
                        </div>
                    </div>
                </div>

            </div>
        }

        <div class="nicdark_space50"></div>

    </div>
    <!--end nicdark_container-->

</section>






