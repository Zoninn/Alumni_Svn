    <script src="~/Content/DesignContent/js/plugins/revslider/jquery.themepunch.revolution.min.js"></script> <!--revslider-->
    <!--menu-->
    <script src="~/Content/DesignContent/js/plugins/menu/superfish.min.js"></script> <!--superfish-->
